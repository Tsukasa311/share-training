<h2 class="liked_user_title">NiceTrainingしたユーザー</h2>
<% post.liked_users.each do |user| %>
  <ul>
    <% if user.profile.image.attached? %>
      <li><%= image_tag user.profile.image, class: "liked_user_image" %></li>
    <% else %>
      <li><%= image_tag "default.png", class: "liked_user_image"%></li>
    <% end %>
    <li class="liked_user_nickname"><%= link_to user.nickname, user_path(user.id) %></li>
  </ul>
<% end %>